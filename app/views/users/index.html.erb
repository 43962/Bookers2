<div class="container px-5 px-sm-0">
 <div class="row">

 <div class="col-md-3">
  <%= render 'userinfo', user: @user %>
  <%= render 'books/newbook', book: @book %>
 </div>

  <div class="col-md-8 offset-md-1">
  <h2>Users</h2>
   <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
      <% @users.each do |user| %>
      <p><%= user.name %></p>
      <% end %>
    </tr>
   </thead>
  </div>
  <tbody>
    <link_to "Show" , user_path[params(:id)] %>
  </tbody>
 </div>
</div>

