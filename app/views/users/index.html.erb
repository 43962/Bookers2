<h2>Users</h2>
<table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <% @users.each do |user| %>

      <p><%= user.name %></p>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <link_to "Show" , user_path[params(:id)] %>
  </tbody>
  <%= render 'userinfo', user: @user %>
  <%= render 'books/newbook', book: @book %>
</table>